@{ 
    string currentval = ViewData["currentValue"] == null ? "" : ViewData["currentValue"] as string;
}
<div id="dateTimeModal">
    <div class="container">
        <div class="form-group">            
            <input type="datetime" class="form-control datetimepicker" id="strDateTime" data-val="@currentval" value="@currentval" tabindex="-1" />
        </div>
    </div>
</div>