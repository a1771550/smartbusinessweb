            <colgroup>
                <col width="130" />
                <col width="210" />
                <col width="220" />
                <col width="140" />
            </colgroup>
            <thead class="gray-header">
                <tr>
                    <th scope="col">@Resources.Resource.Name</th>
                    <th scope="col">@Resources.Resource.Description</th>
                    <th scope="col">@Resources.Resource.Notes</th>
                    <th scope="col"><span class="small">@Resources.Resource.LastUpdateTime</span></th>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
</div>
