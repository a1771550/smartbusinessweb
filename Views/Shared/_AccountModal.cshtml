<div id="accountModal">
    <div class="container">
        <div class="form-inline mb-2 my-4">
            <div class="form-group">
                <input type="text" class="form-control mr-3" id="txtKeyword4Account" placeholder="@Resources.Resource.KeywordRequired" />
                <span class="warning"></span>
            </div>
            <div class="form-group">
                <button class="btn btn-default" id="btnSearchAccount">@Resources.Resource.Search</button>
            </div>
        </div>


        <table id="tblAccount" class="table table-bordered  table-hover table-condensed">
            <colgroup>
                <col width="150" />
                <col width="150" />
                <col width="150" />
                <col width="50" />
            </colgroup>
            <thead>
                <tr class="gray-header">
                    <th><a href="javascript:void(0);" data-category="ItemAccountNo">@Resources.Resource.ItemAccountNo</a></th>
                    <th><a href="javascript:void(0);" data-category="ItemAccountName">@Resources.Resource.ItemAccountName</a></th>
                    <th><a href="javascript:void(0);" data-category="ItemAccountClassification">@Resources.Resource.ItemAccountClassification</a></th>
                    <th></th>
                </tr>
            </thead>

           <tbody></tbody>

        </table>
        <br />
      
        <span id="page"></span>
    </div>
    
</div>
